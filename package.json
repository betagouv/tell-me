{
  "name": "tell-me",
  "description": "An awesome open source survey generator.",
  "version": "0.0.0",
  "license": "AGPL-3.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "16",
    "npm": "8",
    "yarn": "1"
  },
  "scripts": {
    "build": "next build",
    "db:migrate": "prisma migrate deploy",
    "debug": "envinfo --binaries --system --markdown",
    "dev": "concurrently \"next dev\" \"yarn dev:intl\"",
    "dev:docker": "docker-compose down && docker-compose -f ./docker-compose.yml -f ./docker-compose.dev.yml up -d db",
    "dev:intl": "nodemon -e tsx --exec \"yarn intl\" --watch ./app --watch ./pages",
    "dev:migrate": "prisma migrate dev",
    "dev:setup": "sh ./scripts/dev/setup.sh",
    "intl": "yarn intl:extract && node ./scripts/dev/fillLocales.js && yarn intl:compile",
    "intl:compile": "formatjs compile-folder --ast ./locales ./locales/compiled",
    "intl:extract": "formatjs extract \"{app,pages}/**/*.tsx\" --out-file ./locales/en-US.json",
    "prepare": "husky install",
    "prestart": "sh ./scripts/build/prestart.sh",
    "start": "next start",
    "test": "yarn test:lint && yarn test:type && yarn test:unit",
    "test:lint": "eslint --ext cjs,js,jsx,ts,tsx .",
    "test:type": "tsc -p ./tsconfig.dev.json",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=./config/jest.config.js --detectOpenHandles",
    "test:unit:watch": "yarn test:unit --watch"
  },
  "dependencies": {
    "@fontsource/poppins": "4.5.8",
    "@ivangabriele/tsconfig-next": "1.2.2",
    "@popperjs/core": "2.11.5",
    "@prisma/client": "3.15.0",
    "@singularity/core": "4.1.0",
    "@types/multer": "1.4.7",
    "ajv": "8.10.0",
    "babel-plugin-formatjs": "10.3.11",
    "babel-plugin-styled-components": "1.13.2",
    "bhala": "3.0.4",
    "cuid": "2.1.8",
    "dayjs": "1.11.3",
    "dotenv": "16.0.1",
    "formik": "2.2.9",
    "fs-extra": "10.0.0",
    "get-user-locale": "1.4.0",
    "ky": "0.28.5",
    "ky-universal": "0.10.0",
    "lodash": "4.17.21",
    "multer": "1.4.5-lts.1",
    "nexauth": "2.1.1",
    "next": "12.1.6",
    "prisma": "3.15.0",
    "ramda": "0.28.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-feather": "2.0.9",
    "react-intl": "5.24.1",
    "react-popper": "2.3.0",
    "react-toastify": "8.1.0",
    "rehype-parse": "8.0.3",
    "rehype-stringify": "9.0.2",
    "slugify": "1.6.3",
    "styled-components": "5.3.3",
    "ts-node": "10.8.1",
    "typescript": "4.4.4",
    "unified": "10.1.1",
    "unist-builder": "3.0.0",
    "unist-util-reduce": "0.2.2",
    "xlsx": "0.17.4",
    "yup": "0.32.9",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.0",
    "@commitlint/config-conventional": "17.0.0",
    "@formatjs/cli": "4.4.0",
    "@ivangabriele/commitlint-config": "1.0.6",
    "@ivangabriele/eslint-config-typescript-next": "4.1.1",
    "@ivangabriele/prettier-config": "3.0.2",
    "@jest/globals": "28.0.0",
    "@playwright/test": "1.22.2",
    "@swc/core": "1.2.197",
    "@swc/jest": "0.2.21",
    "@types/glob": "7.2.0",
    "@types/jest": "28.1.1",
    "@types/ramda": "0.28.13",
    "@types/styled-components": "5.1.15",
    "@types/zxcvbn": "4.4.1",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "babel-jest": "27.5.1",
    "codecov": "3.8.3",
    "concurrently": "6.3.0",
    "cross-env": "7.0.3",
    "envinfo": "7.8.1",
    "eslint": "8.0.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-formatjs": "2.18.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.0.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "esm-path": "1.0.1",
    "fast-glob": "3.2.11",
    "glob": "7.2.0",
    "husky": "8.0.0",
    "jest": "28.1.1",
    "nodemon": "2.0.14",
    "prettier": "2.5.1"
  },
  "prettier": "@ivangabriele/prettier-config"
}
