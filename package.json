{
  "name": "tell-me",
  "description": "An awesome open source survey generator.",
  "version": "0.0.0",
  "license": "AGPL-3.0",
  "private": true,
  "engines": {
    "node": "16",
    "npm": "8",
    "yarn": "1"
  },
  "scripts": {
    "prebuild": "sh ./scripts/build/prebuild.sh",
    "build": "next build",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:legacy": "ts-node -r dotenv/config ./scripts/db/migrate.ts",
    "debug": "envinfo --binaries --system --markdown",
    "dev": "concurrently \"next dev\" \"yarn dev:intl\"",
    "dev:docker": "docker-compose down && docker-compose -f ./docker-compose.yml -f ./docker-compose.dev.yml up -d db db_legacy",
    "dev:intl": "nodemon -e tsx --exec \"yarn intl\" --watch ./app --watch ./pages",
    "dev:migrate": "prisma migrate dev",
    "intl": "yarn intl:extract && node ./scripts/dev/fillLocales.mjs && yarn intl:compile",
    "intl:compile": "formatjs compile-folder --ast ./locales ./locales/compiled",
    "intl:extract": "formatjs extract \"{app,pages}/**/*.tsx\" --out-file ./locales/en-US.json",
    "prepare": "husky install && node -r dotenv/config ./scripts/dev/setup.mjs",
    "start": "next start",
    "test": "yarn test:lint && yarn test:type && yarn test:unit",
    "test:lint": "eslint --ext js,jsx,mjs,ts,tsx .",
    "test:type": "tsc",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config=./config/jest.config.js --detectOpenHandles",
    "test:unit:watch": "yarn test:unit --watch"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.41.0",
    "@babel/core": "7.15.8",
    "@popperjs/core": "2.10.2",
    "@prisma/client": "3.13.0",
    "@singularity-ui/core": "2.0.0-beta.8",
    "@types/multer": "1.4.7",
    "aws-sdk": "2.1028.0",
    "babel-plugin-formatjs": "10.3.11",
    "babel-plugin-styled-components": "1.13.2",
    "bcryptjs": "2.4.3",
    "better-prop-types": "1.0.0",
    "bhala": "3.0.4",
    "cuid": "2.1.8",
    "dayjs": "1.10.7",
    "dotenv": "10.0.0",
    "formik": "2.2.9",
    "fs-extra": "10.0.0",
    "get-user-locale": "1.4.0",
    "glob": "7.2.0",
    "jose-browser-runtime": "3.17.0",
    "jsonwebtoken": "8.5.1",
    "ky": "0.28.5",
    "ky-universal": "0.10.0",
    "lodash": "4.17.21",
    "mongoose": "6.2.0",
    "multer": "1.4.3",
    "multer-s3": "2.10.0",
    "next": "12.1.5",
    "next-connect": "0.11.0",
    "pem-jwk": "2.0.0",
    "prisma": "3.13.0",
    "ramda": "0.27.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-feather": "2.0.9",
    "react-intl": "5.24.1",
    "react-popper": "2.2.5",
    "react-router-dom": "6.0.2",
    "react-toastify": "8.1.0",
    "rehype": "12.0.0",
    "rehype-parse": "8.0.3",
    "rehype-sanitize": "5.0.0",
    "rehype-stringify": "9.0.2",
    "slugify": "1.6.3",
    "styled-components": "5.3.3",
    "typescript": "4.6.3",
    "unified": "10.1.1",
    "unist-builder": "3.0.0",
    "unist-util-reduce": "0.2.2",
    "xlsx": "0.17.4",
    "yup": "0.32.9",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "@formatjs/cli": "4.4.0",
    "@ivangabriele/commitlint-config": "1.0.6",
    "@ivangabriele/eslint-config-typescript-react": "2.1.4",
    "@ivangabriele/prettier-config": "3.1.0",
    "@jest/globals": "28.0.0",
    "@swc/core": "1.2.171",
    "@types/bcryptjs": "2.4.2",
    "@types/glob": "7.2.0",
    "@types/jest": "27.4.1",
    "@types/ramda": "0.27.46",
    "@types/styled-components": "5.1.15",
    "ajv": "8.10.0",
    "babel-jest": "27.5.1",
    "codecov": "3.8.3",
    "concurrently": "6.3.0",
    "cross-env": "7.0.3",
    "envinfo": "7.8.1",
    "eslint-plugin-formatjs": "2.18.0",
    "eslint-plugin-prettier": "4.0.0",
    "esm-path": "1.0.1",
