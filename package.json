{
  "name": "tell-me",
  "description": "An awesome open source survey generator.",
  "version": "0.0.0",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "debug": "envinfo --binaries --system --markdown",
    "dev": "next dev",
    "dev:docker": "docker-compose down && docker-compose -f ./docker-compose.yml -f ./docker-compose.dev.yml up db",
    "prepare": "husky install && node -r dotenv/config ./scripts/dev/setup.js",
    "start": "next start",
    "test": "yarn test:lint && yarn test:unit",
    "test:lint": "eslint .",
    "test:unit": "jest --maxWorkers=50%"
  },
  "dependencies": {
    "@emotion/react": "11.4.1",
    "@emotion/styled": "11.3.0",
    "@fontsource/mulish": "4.5.1",
    "@mui/icons-material": "5.0.0",
    "@mui/material": "5.0.0-rc.0",
    "@mui/styled-engine-sc": "5.0.0-rc.0",
    "bcryptjs": "2.4.3",
    "bhala": "1.0.0-alpha.1",
    "body-parser": "1.19.0",
    "dayjs": "1.10.7",
    "dotenv": "10.0.0",
    "formik": "2.2.9",
    "jose-browser-runtime": "3.17.0",
    "jsonwebtoken": "8.5.1",
    "ky": "0.28.5",
    "mongoose": "6.0.6",
    "next": "11.1.2",
    "next-transpile-modules": "8.0.0",
    "pem-jwk": "2.0.0",
    "prop-types": "15.7.2",
    "ramda": "0.27.1",
    "react": "17.0.2",
    "react-contenteditable": "3.3.6",
    "react-dom": "17.0.2",
    "react-router-dom": "5.3.0",
    "slugify": "1.6.0",
    "styled-components": "5.3.1",
    "swr": "1.0.1",
    "yup": "0.32.9",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "@ivangabriele/eslint-config-react": "1.5.0",
    "@ivangabriele/prettier-config": "1.1.0",
    "babel-plugin-styled-components": "1.13.2",
    "envinfo": "7.8.1",
    "eslint-plugin-jest": "24.4.0",
    "fs-extra": "10.0.0",
    "husky": "7.0.2",
    "jest": "27.2.0",
    "keypair": "1.0.3",
    "lint-staged": "11.1.2"
  },
  "prettier": "@ivangabriele/prettier-config",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverageFrom": [
      "{api,app-bo,app-fo}/{hooks,libs}/**/*.js",
      "**/helpers/**/*.{js,jsx}"
    ],
    "maxWorkers": "50%"
  }
}
