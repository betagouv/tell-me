{
  "name": "tell-me",
  "description": "An awesome open source survey generator.",
  "version": "0.0.0",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "db:migrate": "babel-node ./scripts/db/migrate.js",
    "debug": "envinfo --binaries --system --markdown",
    "dev": "concurrently \"next dev\" \"yarn dev:intl\"",
    "dev:docker": "docker-compose down && docker-compose -f ./docker-compose.yml -f ./docker-compose.dev.yml up -d db",
    "dev:intl": "nodemon -e tsx --exec \"yarn intl\" --watch ./app --watch ./pages",
    "intl": "yarn intl:extract && node ./scripts/dev/fillLocales.js && yarn intl:compile",
    "intl:compile": "formatjs compile-folder --ast ./locales ./locales/compiled",
    "intl:extract": "formatjs extract \"{app,pages}/**/*.tsx\" --out-file ./locales/en-US.json",
    "prepare": "husky install && node -r dotenv/config ./scripts/dev/setup.js",
    "start": "yarn db:migrate && next start",
    "test": "yarn test:lint && yarn test:type && yarn test:unit",
    "test:lint": "eslint --ext js,jsx,ts,tsx .",
    "test:type": "tsc",
    "test:unit": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@babel/core": "7.15.8",
    "@babel/node": "7.15.8",
    "@fontsource/poppins": "4.5.0",
    "@popperjs/core": "2.10.2",
    "@singularity-ui/core": "1.9.0",
    "babel-plugin-formatjs": "10.3.11",
    "babel-plugin-styled-components": "1.13.2",
    "bcryptjs": "2.4.3",
    "bhala": "2.0.2",
    "dayjs": "1.10.7",
    "dotenv": "10.0.0",
    "formik": "2.2.9",
    "fs-extra": "10.0.0",
    "get-user-locale": "1.4.0",
    "glob": "7.2.0",
    "jose-browser-runtime": "3.17.0",
    "jsonwebtoken": "8.5.1",
    "ky": "0.28.5",
    "lodash": "4.17.21",
    "mongoose": "6.0.6",
    "multer": "1.4.3",
    "next": "11.1.2",
    "pem-jwk": "2.0.0",
    "prop-types": "15.7.2",
    "ramda": "0.27.1",
    "react": "17.0.2",
    "react-contenteditable": "3.3.6",
    "react-dom": "17.0.2",
    "react-feather": "2.0.9",
    "react-intl": "5.21.0",
    "react-popper": "2.2.5",
    "react-router-dom": "5.3.0",
    "react-spreadsheet": "0.6.0",
    "rehype": "12.0.0",
    "rehype-parse": "8.0.3",
    "rehype-sanitize": "5.0.0",
    "rehype-stringify": "9.0.2",
    "slugify": "1.6.0",
    "swr": "1.0.1",
    "typescript": "4.4.4",
    "unified": "10.1.0",
    "unist-util-reduce": "0.2.2",
    "xlsx": "0.17.3",
    "yup": "0.32.9",
    "zxcvbn": "4.4.2"
  },
  "devDependencies": {
    "@formatjs/cli": "4.4.0",
    "@ivangabriele/eslint-config-react": "1.5.0",
    "@ivangabriele/eslint-config-typescript-react": "1.5.0",
    "@ivangabriele/prettier-config": "1.1.0",
    "@types/jest": "27.0.2",
    "@types/ramda": "0.27.46",
    "babel-jest": "27.2.4",
    "concurrently": "6.3.0",
    "envinfo": "7.8.1",
    "eslint-plugin-formatjs": "2.18.0",
    "eslint-plugin-jest": "24.4.0",
    "husky": "7.0.2",
    "jest": "27.2.0",
    "keypair": "1.0.4",
    "lint-staged": "11.1.2",
    "nodemon": "2.0.14"
  },
  "prettier": "@ivangabriele/prettier-config",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
